super + Return
    urxvt

super + {_,ctrl + ,alt + }{Up,Down,Left,Right}
    custard - window {move,expand,contract} {north,south,west,east}

super + shift + {Up,Down}
    custard - window {raise,lower}

super + {1-9,0}
    custard - workspace {1-9,10}

super + w
    custard - window close

super + Escape
    custard - halt
